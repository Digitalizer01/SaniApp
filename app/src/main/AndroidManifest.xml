<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.saniapp" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_apk"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SaniApp" >
        <activity
            android:name=".AdminActivity"
            android:exported="false" />
        <activity
            android:name=".ResidenceActivity"
            android:exported="false"
            android:label="@string/title_activity_residence"
            android:theme="@style/Theme.SaniApp.NoActionBar" />
        <activity
            android:name=".UserActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>